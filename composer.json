{
    "name": "gammamatrix/playground-matrix",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Database\\Factories\\GammaMatrix\\Playground\\Matrix\\Models\\": "database/factories/",
            "GammaMatrix\\Playground\\Matrix\\": "src/"
